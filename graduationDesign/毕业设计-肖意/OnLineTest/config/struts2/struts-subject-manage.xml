<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.1.7//EN"
	"http://struts.apache.org/dtds/struts-2.1.7.dtd">
<struts>
  <package name="subjectManage" extends="struts-default" namespace="/teacher">
  	<action name="subjectManageAction_*" method="{1}" class="subjectManageAction">
  		<result name="success">/WEB-INF/teacher/subjectManage.jsp</result>
  		<result name="question">/WEB-INF/teacher/questionManage.jsp</result>
  	</action>
  </package>
  
  
	  <package name="student_subject" extends="struts-default" namespace="/student">
  		<action name="subjectManageAction_*" method="{1}" class="subjectManageAction">
  			<result name="success">/WEB-INF/student/onLineTest.jsp</result>
  			<result name="questions">/WEB-INF/student/questions.jsp</result>
  	</action>
  </package>
  
  
  
  
  
</struts>	